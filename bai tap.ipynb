    "# Bước 1: Import thư viện turtle\n",
    "import turtle\n",
    "\n",
    "# Bước 2: Tạo đối tượng turtle\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "t.pencolor('#0000ff')   #RGB\n",
    "t.pensize(10)\n",
    "t.speed(1)\n",
    "#b3\n",
    "\n",
    "a = 150\n",
    "t.forward(a)\n",
    "t.left(120)\n",
    "t.forward(a)\n",
    "t.left(120)\n",
    "t.forward(a)\n",
    "\n",
    "\n",
    "p = a * 3\n",
    "\n",
    "print(p)\n"
   ]
  }
