    "import turtle\n",
    "import math\n",
    "\n",
    "\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "#t.pencolor('#0000ff')   #RGB\n",
    "#t.pensize(10)\n",
    "t.speed(1)\n",
    "\n",
    "\n",
    "\n",
    "turtle.circle (100)\n",
    "r = 100\n",
    "s = math.pi * r*2\n",
    "print (s)\n",
    "turtle.done()"
   ]
  }
